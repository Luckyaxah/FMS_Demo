{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "project": {
    "dataSource": "mysqlDB",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "find": true
        }
      }
    }
  },
  "record": {
    "dataSource": "mysqlDB",
    "public": true,
    "options":{
      "remoting":{
        "sharedMethods":{
          "*":false,
          "find":true,
          "initData":false,
          "findByProjectIdM":true
        }
      }
    }
  }
}
